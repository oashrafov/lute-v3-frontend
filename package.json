{
  "name": "lute-v3-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "imports": {
    "#book/*": [
      "./src/features/book/*",
      "./src/features/book/*.ts",
      "./src/features/book/*.tsx"
    ],
    "#language/*": [
      "./src/features/language/*",
      "./src/features/language/*.ts",
      "./src/features/language/*.tsx"
    ],
    "#term/*": [
      "./src/features/term/*",
      "./src/features/term/*.ts",
      "./src/features/term/*.tsx"
    ],
    "#settings/*": [
      "./src/features/settings/*",
      "./src/features/settings/*.ts",
      "./src/features/settings/*.tsx"
    ],
    "#pages/*": "./src/pages/*.tsx",
    "#resources/*": [
      "./src/resources/*",
      "./src/resources/*.ts",
      "./src/resources/*.tsx"
    ],
    "#common/*": "./src/components/common/*.tsx",
    "#components/*": "./src/components/*.tsx",
    "#hooks/*": "./src/hooks/*.ts",
    "#utils/*": "./src/utils/*.ts",
    "#helpers/*": "./src/helpers/*.ts"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@mantine/charts": "^7.17.0",
    "@mantine/core": "^7.17.0",
    "@mantine/dates": "^7.17.0",
    "@mantine/hooks": "^7.17.0",
    "@mantine/modals": "^7.17.0",
    "@mantine/notifications": "^7.17.0",
    "@mantine/nprogress": "^7.17.0",
    "@tabler/icons-react": "3.17.0",
    "@tanstack/react-query": "^5.90.3",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.132.47",
    "@tanstack/react-router-devtools": "^1.133.1",
    "dayjs": "^1.11.13",
    "i18next": "^25.5.2",
    "mantine-react-table": "^2.0.0-beta.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.3",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@tanstack/eslint-plugin-query": "^5.64.2",
    "@tanstack/router-plugin": "^1.132.56",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "prettier": "3.3.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6"
  }
}
